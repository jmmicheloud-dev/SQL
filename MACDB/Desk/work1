SELECT * FROM salas

SELECT * FROM tesorerias 
WHERE sala = '29' ORDER BY id DESC 
LIMIT 10

SELECT * FROM tesoreria_movs
WHERE sala = '29'ORDER BY id DESC 
LIMIT 50

SELECT * FROM tes_admin_movs
WHERE sala = '34'ORDER BY id DESC 
LIMIT 50

SELECT * FROM tesoreria_tipos WHERE id = '2'

SELECT * FROM rendicions
WHERE sala = '4' ORDER BY id DESC 
LIMIT 10

SELECT * FROM rendicion_tipos
WHERE sala = '29' ORDER BY id DESC 
LIMIT 10

SELECT * FROM cajas 

WHERE sala = '29' ORDER BY id DESC 
LIMIT 10

GROUP BY estado

