SELECT * FROM tesorerias WHERE 1 ORDER BY id DESC LIMIT 10

SELECT * FROM tesorerias WHERE sala = '2' ORDER BY id DESC LIMIT 10

SELECT * FROM tesorerias WHERE sala = '26' ORDER BY id DESC LIMIT 10

SELECT * FROM tesoreria_movs WHERE sala = '26' ORDER BY id DESC LIMIT 100

